{
  "cli": {
    "version": ">= 16.9.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_ENDPOINT": "https://v2-vercel-passbit-api.vercel.app/"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_ENDPOINT": "https://v2-vercel-passbit-api.vercel.app/"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_API_ENDPOINT": "https://v2-vercel-passbit-api.vercel.app/"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
