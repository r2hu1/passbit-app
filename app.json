{
  "expo": {
    "name": "passbit-app",
    "slug": "passbit-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "fonts": [
      "./assets/font/Rubik-Black.ttf",
      "./assets/font/Rubik-Bold.ttf",
      "./assets/font/Rubik-Regular.ttf",
      "./assets/font/Rubik-Medium.ttf",
      "./assets/font/Rubik-SemiBold.ttf",
      "./assets/font/Rubik-Bold.ttf",
      "./assets/font/Rubik-ExtraBold.ttf"
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.passbit.r2hu1.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "06ef3167-f808-4b56-80ed-eece7c96dbc6"
      }
    }
  }
}
